{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "Get web page source from a popup",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
     "matches":["<all_urls>"],
     "js": ["jquery-3.2.1.min.js"]
   }],

  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "https://ajax.googleapis.com/",
    "tabs"
  ]
}
